{
  "name": "@all41-dev/front.lib",
  "version": "1.0.18",
  "description": "Stencil UI lib",
  "main": "dist/index.cjs.js",
  "module": "dist/index.js",
  "es2015": "dist/esm/index.mjs",
  "es2017": "dist/esm/index.mjs",
  "types": "dist/types/index.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "collection:main": "dist/collection/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/all41-dev/front.lib.git"
  },
  "files": [
    "dist/",
    "loader/"
  ],
  "homepage": "https://github.com/all41-dev/front.lib.git#readme",
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  },
  "scripts": {
    "build": "stencil build --docs --max-old-space-size=8192",
    "build.prerender": "stencil build --prerender",
    "start": "stencil build --dev --watch --serve --max-old-space-size=8192",
    "test": "stencil test --spec --e2e",
    "test.watch": "stencil test --spec --e2e --watchAll --port 3334",
    "generate": "stencil generate",
    "preversion": "npm run build --prod",
    "postversion": "git push && git push --tags && npm publish"
  },
  "dependencies": {
    "@codemirror/lang-javascript": "^6.1.9",
    "@popperjs/core": "^2.11.6",
    "@stencil/core": "^4.21.0",
    "@stencil/store": "^2.0.16",
    "@types/uuid": "^9.0.7",
    "ace-builds": "^1.35.2",
    "bootstrap": "^5.3.3",
    "codemirror": "^6.0.1",
    "cron-parser": "^4.9.0",
    "d3": "^7.9.0",
    "highlight.js": "^11.10.0",
    "pluralize": "^8.0.0",
    "sweetalert2": "^11.18.0",
    "tabulator-tables": "5.5.2",
    "typeahead.js": "^0.11.1",
    "vanilla-jsoneditor": "^0.17.8",
    "chart.js": "^4.4.3",
    "chartjs-adapter-date-fns": "^3.0.0",
    "chartjs-adapter-luxon": "^1.3.1",
    "chartjs-plugin-annotation": "^3.0.1",
    "chartjs-plugin-datalabels": "^2.2.0"
  },
  "devDependencies": {
    "@types/bootstrap": "^5.2.6",
    "@types/jest": "^29.4.0",
    "@types/jquery": "^3.5.29",
    "@types/luxon": "^3.3.4",
    "@types/markdown-it": "^13.0.7",
    "@types/pluralize": "0.0.29",
    "@types/tabulator-tables": "5.4.4",
    "@types/typeahead.js": "^0.11.6",
    "humanize-duration": "^3.28.0",
    "jest": "^29.4.2",
    "jest-cli": "^29.4.2",
    "luxon": "^3.4.4",
    "puppeteer": "^24.2.1",
    "pure-uuid": "^1.6.2",
    "rollup-plugin-node-polyfills": "^0.2.1"
  },
  "license": ""
}
